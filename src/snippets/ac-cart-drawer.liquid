<div id="DrawerOverlay"></div>
<div id="CartDrawer" class="c-cart-drawer is-right">

  <div class="c-cart-drawer__header">
    <div class="c-cart-drawer__heading">
      <span class="c-cart-drawer__title" >{{ 'cart.general.title' | t }}</span>
    </div>
    <div class="c-cart-drawer__icon--close js-drawer-close">
      {% include "icon-close" %}
      <span class="icon-fallback-text">X</span>
    </div>
  </div>
  <div id="CartContainer" class="c-cart-drawer__cart" ></div>

</div>
